@model SensenbrennerHospital.Models.VolunteerPositionDto
<head>
    // Stylesheets
    //
    <link rel="stylesheet" href="~/Content/Volunteers/articles.css">
    //
    <link rel="stylesheet" href="~/Content/Site.css">
</head>

<h2>View</h2>

<h3>Are you sure you want to delete this Volunteer Position?</h3>
<div>
    <dl class="dl-horizontal">

        <dt>
            @Html.DisplayNameFor(model => model.Name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Description)
        </dd>

    </dl>

    @using (Html.BeginForm("Delete", "VolunteerPosition", new { id = Model.CvpID }))
    {
        @Html.AntiForgeryToken()

        <div class="options1">
            <span class="editlink"><input type="submit" value="Delete" /></span>
            <span class="editlink"> @Html.ActionLink("Back to List", "List")</span>
        </div>
    }
</div>